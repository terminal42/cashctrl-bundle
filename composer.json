{
    "name": "terminal42/cashctrl-bundle",
    "type": "symfony-bundle",
    "description": "Symfony bundle to the API client for cashctrl.com",
    "license": "MIT",
    "authors": [
        {
            "name": "terminal42 gmbh",
            "homepage": "https://www.terminal42.ch/"
        }
    ],
    "funding": [
        {
            "type": "github",
            "url": "https://github.com/terminal42"
        },
        {
            "type": "other",
            "url": "https://ko-fi.com/terminal42"
        }
    ],
    "require": {
        "php": "^8.2",
        "terminal42/cashctrl-api": "^2.0@dev",
        "symfony/http-kernel": "^5.0 || ^6.0 || ^7.0",
        "symfony/dependency-injection": "^5.0 || ^6.0 || ^7.0",
        "symfony/config": "^5.0 || ^6.0 || ^7.0",
        "symfony/http-client": "^5.0 || ^6.0 || ^7.0",
        "nyholm/psr7": "^1.0"
    },
    "require-dev": {
        "terminal42/contao-build-tools": "dev-main"
    },
    "autoload": {
        "psr-4": {
            "Terminal42\\CashctrlBundle\\": "src"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-main": "2.0.x-dev",
            "dev-php7": "1.0.x-dev"
        }
    },
    "config": {
        "allow-plugins": {
            "terminal42/contao-build-tools": true
        }
    }
}
